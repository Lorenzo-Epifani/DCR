        	}
	)
db.pic.insertOne({ 

		"rel" : "./castellohimeji.jpeg", "name": "Himeji Castle", "class" : "Castle", "Country" : "Japan", "City" : "Himeji", "height" : "46.4 m",
		"additional_info" : "Tower, door, wall, fort", "colur" : "white grey", 
        "description" : "Himeji Castle is a hilltop Japanese castle complex situated in the city of Himeji which 
		is located in the Hyōgo Prefecture of Japan. The castle is regarded as the finest surviving example of prototypical Japanese 
		castle architecture, comprising a network of 83 rooms with advanced defensive systems from the feudal period. The castle is 
		frequently known as Hakuro-jō or Shirasagi-jō ("White Egret Castle" or "White Heron Castle") because of its brilliant white 
		exterior and supposed resemblance to a bird taking flight."
       
        	}
	)
db.pic.insertOne({ 

		"rel" : "./brandeburgo.jpeg", "name": "Brandenburg Gate", "class" : "Gate", "Country" : "Germany", "City" : "Brandenburg", "height" : "46.4 m",
		"additional_info" : "Port, door, columns, statue", "colur" : "yellow grey", 
        "description" : "The Brandenburg Gate is an 18th-century neoclassical monument in Berlin, built on the orders of Prussian 
		king Frederick William II after the (temporarily) successful restoration of order during the early Batavian Revolution.
