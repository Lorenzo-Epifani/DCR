db.pic.insertOne({ 

		"rel" : "./chiesasannavilnius.jpeg", "name": "St. Anne's Church", "class" : "Gothic Church", "Country" : "Lithuanian", "City" : "Vilnius", "height" : "",
		"additional_info" : "Tower, cross, columns, door, bell tower", "colur" : "rust red", 
        "description" : "St. Anne's Church is a Roman Catholic church in Vilnius' Old Town, on the right bank of the Vilnia River. It is a prominent 
