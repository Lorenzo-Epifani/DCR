        	}
	)
db.pic.insertOne({ 

		"rel" : "./bigben.jpeg", "name": "Big Ben", "class" : "Tower", "Country" : "United Kingdom", "City" : "London", "height" : "96 m",
		"additional_info" : "Tower, door, clock", "colur" : "yellow grey", 
        "description" : "Big Ben is the nickname for the Great Bell of the clock at the north end of the Palace of Westminster 
		in London and is usually extended to refer to both the clock and the clock tower. The official name of the tower in which 
